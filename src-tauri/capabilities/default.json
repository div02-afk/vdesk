{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "platforms": ["windows"],
  "permissions": [
    "core:default",
    "opener:default",
    "fs:default",
    {
      "identifier": "fs:allow-write-file",
      "paths": ["$APPDATA/*"]
    },
    {
      "identifier": "fs:allow-read-file",
      "paths": ["$APPDATA/*"]
    }
  ]
}
